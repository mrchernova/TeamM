<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC  "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"  "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="true">





  <request-character-encoding>UTF-8</request-character-encoding>




  <servlet>
    <servlet-name>Health</servlet-name>
    <servlet-class>by.trucking.HealthServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Health</servlet-name>
    <url-pattern>/health</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>getAllOrders</servlet-name>
    <servlet-class>by.trucking.controller.GetAllOrders</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>getAllOrders</servlet-name>
    <url-pattern>/go</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>spgg</servlet-name>
    <servlet-class>by.trucking.controller.GetOrderById</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>spgg</servlet-name>
    <url-pattern>/bot</url-pattern>
  </servlet-mapping>


</web-app>
